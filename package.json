{
  "name": "redux",
  "version": "0.0.1",
  "description": "Just a simple redux app",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Freezystem/redux-app.git"
  },
  "esdoc": {
    "source": "./app",
    "destination": "./docs",
    "test": {
      "type": "mocha",
      "source": "./test",
      "includes": [
        "\\.specs\\."
      ]
    }
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "react-html-attrs",
      "transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^0.9.0",
    "redux-observable": "^0.12.2",
    "rxjs": "^5.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.7.0",
    "esdoc": "^0.5.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.2",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "react-addons-test-utils": "^15.4.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "clean-install": "rm -rf ./node_modules && npm cache clean && npm i",
    "dev": "webpack-dev-server --content-base dist --inline --hot --port 3004 --host redux.app.io --open",
    "build": "export NODE_ENV=production&& npm i && webpack -p",
    "build:clean": "export NODE_ENV=production&& npm run clean-install && webpack -p",
    "test": "mocha --compilers js:babel-register --recursive --es_staging --use_strict -r ignore-styles -r jsdom-global/register",
    "test:watch": "npm test -- --watch",
    "doc": "esdoc"
  },
  "author": "freezystem@gmail.com",
  "license": "UNLICENSED"
}
